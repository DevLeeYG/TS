{
  "compilerOptions": {
  "module": "commonjs",
  "target": "es2016",                                  
  "moduleResolution": "node",
  "outDir": "dist",
  "esModuleInterop": true,                             
  "baseUrl": ".",
  "sourceMap": true,
  "downlevelIteration": true,
  "noImplicitAny": false,
  "forceConsistentCasingInFileNames": true,         
  "strict": true,                                    
  "skipLibCheck": true, 
  "paths": {"*":["node_modules/*"]}                           
},
"include": ["src/**/*"]
}
